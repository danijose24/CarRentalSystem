<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
 
 <hibernate-mapping>
    <class name="com.i2i.model.Car" table="Car">
      <meta attribute="class-description">
         This class contains the Car detail. 
      </meta>
      
      <id name="id" type="int" column="id">
            <generator class="increment"/>
        </id>
      
      <property name="carType" column="carType" type="int"/>
      <property name="carNumber" column="carNumber" type="string"/>
          
      <set name="bookings" table="Booking" inverse="true" lazy="false" fetch="select">
            <key>
                <column name="id" not-null="false"/>
            </key>
            <one-to-many class="com.i2i.model.Booking"/>
      </set>

      <many-to-one name="make" class="com.i2i.model.Make" lazy="false" fetch="select">
            <column name="makeId"/>
      </many-to-one>
  </class>
 </hibernate-mapping>
