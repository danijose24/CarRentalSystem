<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.i2i.model.Booking" table="Booking">
        <meta attribute="class-description">
        This class contains the booking detail.
        </meta>
        <id name="id" type="int" column="id">
            <generator class="increment"/>
        </id>
        <property name="pickupDate" column="pickupDate" type="date"/>
        <property name="dropDate" column="dropDate" type="date"/>
        <property name="amount" column="amount" type="int"/>
        <many-to-one name="user" class="com.i2i.model.User" lazy="false" fetch="select">
            <column name="userId" not-null="false"/>
        </many-to-one>
        <many-to-one name="car" class="com.i2i.model.Car" lazy="false" fetch="select">
            <column name="carId" not-null="false"/>
        </many-to-one>
    </class>
</hibernate-mapping>
